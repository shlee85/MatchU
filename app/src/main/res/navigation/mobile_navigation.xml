<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.lowasis.matchu.ui.feed.FeedFragment"
        android:label="홈"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.lowasis.matchu.ui.nearFragment.NearFragment"
        android:label="주변찾기"
        tools:layout="@layout/fragment_near" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.lowasis.matchu.ui.notifications.NotificationsFragment"
        android:label="공지사항"
        tools:layout="@layout/fragment_notifications" />
</navigation>